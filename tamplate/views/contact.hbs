        <!-- title bar start -->
        <div class="title-bar">
          <h1>Contact Us</h1>
        </div>
        <!-- title bar end -->
        <!-- conatct start -->
      <div class="contact">
        <div class="container">
          <div class="contact-card-section">
            <div class="contact-card">
              <ul class="contact-card-list d-flex align-items-center justify-content-between">
                <li class="contact-card-list-items">
                  <div class="contact-card-info">
                    <div class="contact-card-title">
                      <h4>EMAIL</h4>
                      <p><a href="#">info@zillaonline.com</a></p>
                      <p><a href="#">zilla24@gmail.com</a></p>
                    </div>
                  </div>
                </li>
                <li class="contact-card-list-items">
                  <div class="contact-card-info info2">
                    <div class="contact-card-title">
                      <h4>Location</h4>
                      <p>100 N Aurora Ave #APT 19</p>
                      <p>Okland, Nebraska(NE), 68045 </p>
                    </div>
                  </div>
                </li>
                <li class="contact-card-list-items ">
                  <div class="contact-card-info info3">
                    <div class="contact-card-title">
                      <h4>Phone</h4>
                      <p><a href="#">(000) 123-4567</a></p>
                      <p><a href="#">+11 (00) 2233 444 555</a></p>
                    </div>
                  </div>
                </li>
              </ul>
            </div>
          </div>
          <div class="contact-form-section">
            <div class="contact-form-info">
              <div class="contact-form-title">
                <h4>Get In Touch</h4>
              </div>
              <div class="contact-form-description">
                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor <br>
                  incididunt ut labore et dolore magna aliqua. </p>
              </div>
            </div>
            <div class="contact-form">
              <form>
                <div class="input-feild">
                  <input type="text" placeholder="Name">
                  <input type="text" placeholder="Email">
                </div>
                <div class="input-feild">
                  <input type="text" placeholder="Phone">
                  <input type="text" placeholder="Subject">
                </div>
                <div class="input-feild">
                  <textarea name="" id="" cols="30" rows="7" placeholder="Message"></textarea>
                </div>
                <button>Submit</button>
              </form>
            </div>
          </div>
        </div>
      </div>
       <!-- Contact end -->
       <!-- Map start -->
       <div class="map">
        <div class="mapouter"><div class="gmap_canvas"><iframe class="gmap_iframe" width="100%" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?width=600&amp;height=400&amp;hl=en&amp;q=usa&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed"></iframe><a href="https://piratebay-proxys.com/">Piratebay</a></div><style>.mapouter{position:relative;text-align:right;width:100%;height:400px;}.gmap_canvas {overflow:hidden;background:none!important;width:100%;height:400px;}.gmap_iframe {height:400px!important;}</style></div>
       </div>
       <!-- Map End -->
    </main>
      <!-- Main page end -->
      <div class="pop">
        <div class="overlay" id="overlay-id">
          <div class="popup">
            <div class="close-popup"><img src="webimages/logo/close-btn.png" alt=""></div>
            <div class="pop-body">
              <div class="form-title">
                <img src="webimages/logo/door-key.png" alt="">
                <h3>Sign In</h3>
              </div>
              <form >
                <div class="input-feild"><img src="webimages/logo/user.png" alt=""> <input type="text" name="" id="" placeholder="Username" ></div>
                <div class="input-feild"><img src="webimages/logo/lock-in.png" alt=""> <input type="text" name="" id="" placeholder="Password"></div>
              </form>
              <div class="form-btn">
                <a href="#"><img src="webimages/logo/signIn-btn.png" alt=""></a>
              </div>
            
            </div>
          </div>
          <div class="account-link">
            <a href="#" class="signup login-acc">Don't Have an Account? <span>SignUP</span> </a>
          </div>
        </div>
        <div class="SignUP">
          <div class="popup">
            <div class="close-popup"><img src="webimages/logo/close-btn.png" alt=""></div>
            <div class="pop-body">
              <div class="form-title">
                <img src="webimages/logo/door-key.png" alt="">
                <h3>Sign Up</h3>
              </div>
              <form >
                <div class="input-feild"><img src="webimages/logo/user.png" alt=""> <input type="text" name="" id="" placeholder="Name" ></div>
                <div class="input-feild"><img src="images/email.png" alt=""> <input type="text" name="" id="" placeholder="Email"></div>
                <div class="input-feild"><img src="images/mobile-phone.png" alt=""> <input type="text" name="" id="" placeholder="Phone" ></div>
                <div class="input-feild"><img src="images/icon-ios7-eye-512.png" alt=""> <input type="text" name="" id="" placeholder="Password"></div>
                <div class="input-feild"><img src="images/icon-ios7-eye-512.png" alt=""> <input type="text" name="" id="" placeholder="Confirm Password" ></div>
              
              </form>
              <div class="form-btn">
                <a href="#"><img src="webimages/logo/signIn-btn.png" alt=""></a>
              </div>
             
            </div>
          </div>
          <div class="account-link">
            <a href="#" class="button signup-acc">already Have an Account? <span>Login</span> </a>
          </div>
        </div>
      </div>
      <!-- Footer start -->
     {{>footer}}
